    "@tabler/icons-react": "^3.35.0",
    "opening_hours": "^3.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^5.0.0",
    "jsdom": "^27.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vitest": "^1.6.0"
  }
